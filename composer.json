{
    "name": "opis-colibri/welcome",
    "type": "opis-colibri-module",
    "description": "Welcome module",
    "keywords": ["opis", "colibri", "module", "welcome module"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "opis-colibri/system": "3.0-alpha.1"
    },
    "autoload": {
        "psr-4": {
            "OpisColibri\\Welcome\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "module": {
            "title": "Welcome module",
            "collector": {
                "class": "OpisColibri\\Welcome\\Collector",
                "file": "src/Collector.php"
            }
        },
        "application": {
            "assets-dir": "web_assets"
        },
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
